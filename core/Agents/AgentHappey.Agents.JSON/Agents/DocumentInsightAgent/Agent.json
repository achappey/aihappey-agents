{
    "name": "DocumentInsightAgent",
    "description": "Provides deep insights into documents through reading, extraction, comparison, highlighting and analysis.",
    "instructions": "Use only the connected document-analysis capabilities to read, interpret, compare and extract information. Ground all conclusions strictly in the content retrieved through the MCP servers, and avoid unsupported assumptions or external knowledge.",
    "model": {
        "id": "openai/gpt-5.2",
        "options": {
            "temperature": 1
        },
        "providerMetadata": {
            "include": [
                "web_search_call.action.sources",
                "reasoning.encrypted_content",
                "code_interpreter_call.outputs",
                "file_search_call.results"
            ],
            "parallel_tool_calls": true,
            "file_search": null,
            "native_mcp": false,
            "code_interpreter": null,
            "reasoning": {
                "effort": "low",
                "summary": "auto"
            }
        }
    },
    "mcpServers": {
        "documenttools": {
            "type": "http",
            "url": "/DocumentTools"
        },
        "documentsummarizer": {
            "type": "http",
            "url": "/DocumentSummarizer"
        },
        "documentqna": {
            "type": "http",
            "url": "/DocumentQnA"
        },
        "documenthighlighter": {
            "type": "http",
            "url": "/DocumentHighlighter"
        },
        "documentcomparer": {
            "type": "http",
            "url": "/DocumentComparer"
        },
        "edenai-text": {
            "type": "http",
            "url": "/EdenAI-Text"
        }
    },
    "mcpClient": {
        "capabilities": {
            "sampling": {}
        },
        "policy": {
            "readOnlyHint": true,
            "openWorldHint": true,
            "destructiveHint": false,
            "idempotentHint": false
        }
    }
}