{
    "name": "CapabilityAgent",
    "description": "Full-capability agent for testing tools, workflows and behaviors safely.",
    "instructions": "You are Capability Agent. Act as a full-capability sandbox agent: exercise all capabilities, sampling, elicitation, workflows, and toolcalls aggressively for testing, but never assume real-world consequences.",
    "model": {
        "id": "openai/gpt-5.2",
        "options": {
            "temperature": 1
        },
        "providerMetadata": {
            "include": [
                "web_search_call.action.sources",
                "reasoning.encrypted_content",
                "code_interpreter_call.outputs",
                "file_search_call.results"
            ],
            "parallel_tool_calls": true,
            "file_search": null,
            "native_mcp": false,
            "code_interpreter": {},
            "reasoning": {
                "effort": "low",
                "summary": "auto"
            }
        }
    },
    "outputSchema": {
        "properties": {
            "language": {
                "type": "string",
                "required": true,
                "description": "The language of the answer."
            },
            "answer": {
                "type": "string",
                "required": true,
                "description": "The answer text."
            }
        }
    },
    "mcpServers": {
        "modelcontext-utils": {
            "type": "http",
            "url": "/ModelContext-Utils"
        },
        "github-jsonplaceholder": {
            "type": "http",
            "url": "/GitHub-JSONPlaceholder"
        },
        "github-jsonblob": {
            "type": "http",
            "url": "/GitHub-JSONBlob"
        },
        "github-netbarcode": {
            "type": "http",
            "url": "/GitHub-NetBarcode"
        },
        "github-humanizer": {
            "type": "http",
            "url": "/GitHub-Humanizer"
        },
        "github-bogus": {
            "type": "http",
            "url": "/GitHub-Bogus"
        },
        "websearch": {
            "type": "http",
            "url": "/WebSearch"
        }
    },
    "mcpClient": {
        "capabilities": {
            "sampling": {},
            "elicitation": {}
        },
        "policy": {
            "readOnlyHint": false,
            "openWorldHint": true,
            "destructiveHint": true,
            "idempotentHint": false
        }
    }
}