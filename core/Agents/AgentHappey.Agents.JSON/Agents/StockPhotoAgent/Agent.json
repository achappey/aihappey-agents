{
    "name": "StockPhotoAgent",
    "description": "Searches and retrieves high-quality stock photos from connected photo libraries for use in presentations, documents and creative work.",
    "instructions": "Use only the available stock-photo capabilities to search, filter and select relevant images. Do not generate or modify images. Base all results strictly on the photos retrieved through these MCP servers, and avoid unsupported assumptions about licensing or content.",
    "model": {
        "id": "openai/gpt-5.2",
        "options": {
            "temperature": 1
        },
        "providerMetadata": {
            "include": [
                "web_search_call.action.sources",
                "reasoning.encrypted_content",
                "code_interpreter_call.outputs",
                "file_search_call.results"
            ],
            "parallel_tool_calls": true,
            "file_search": null,
            "native_mcp": false,
            "code_interpreter": null,
            "reasoning": {
                "effort": "low",
                "summary": "auto"
            }
        }
    },
    "mcpServers": {
        "pexels": {
            "type": "http",
            "url": "/Pexels"
        },
        "unsplash": {
            "type": "http",
            "url": "/Unsplash"
        }
    },
    "mcpClient": {
        "capabilities": {
            "sampling": {}
        },
        "policy": {
            "readOnlyHint": true,
            "openWorldHint": true,
            "destructiveHint": false,
            "idempotentHint": false
        }
    }
}
