{
    "name": "DateTimeAgent",
    "description": "Provides accurate time, date, calendar, timezone and astronomical information using the connected capabilities, supporting conversions, lookups and temporal reasoning.",
    "instructions": "Use only the available date- and time-related capabilities for all lookups, calculations and reasoning. Ground every answer strictly in data retrieved through these MCP servers, and avoid unsupported assumptions about regional rules or time standards. You may use web search to confirm time-sensitive context when needed, but ensure all conclusions remain based on verifiable results. Do not modify external systems. This agent is for reference and analysis only.",
    "model": {
        "id": "openai/gpt-5.2",
        "options": {
            "temperature": 1
        },
        "providerMetadata": {
            "include": [
                "web_search_call.action.sources",
                "reasoning.encrypted_content",
                "code_interpreter_call.outputs",
                "file_search_call.results"
            ],
            "parallel_tool_calls": true,
            "file_search": null,
            "native_mcp": false,
            "code_interpreter": null,
            "reasoning": {
                "effort": "low",
                "summary": "auto"
            },
            "web_search": {
                "search_context_size": "low",
                "user_location": {
                    "type": "approximate",
                    "city": null,
                    "region": null,
                    "country": null,
                    "timezone": null
                }
            }
        }
    },
    "mcpServers": {
        "nagerdate": {
            "type": "http",
            "url": "/NagerDate"
        },
        "stedin-eklok": {
            "type": "http",
            "url": "/Stedin-eKlok"
        },
        "sunrisesunset": {
            "type": "http",
            "url": "/SunriseSunset"
        },
        "worldtimeapi": {
            "type": "http",
            "url": "/WorldTimeAPI"
        },
        "calendar": {
            "type": "http",
            "url": "/Calendar"
        },
        "github-nodatime": {
            "type": "http",
            "url": "/GitHub-NodaTime"
        }
    },
    "mcpClient": {
        "capabilities": {
            "sampling": {}
        },
        "policy": {
            "readOnlyHint": true,
            "openWorldHint": true,
            "destructiveHint": false,
            "idempotentHint": false
        }
    }
}